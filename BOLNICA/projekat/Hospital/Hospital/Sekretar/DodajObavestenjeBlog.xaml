<Window
    x:Class="Hospital.Sekretar.DodajObavestenjeBlog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Hospital.Sekretar"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="DodajObavestenjeBlog"
    Width="400"
    Height="300"
    mc:Ignorable="d">
    <Grid>
        <TextBox
            Name="sadrzaj"
            Width="241"
            Height="114"
            Margin="71,55,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Text=""
            TextWrapping="Wrap" />
        <Label
            Margin="71,24,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Content="Sadrzaj" />
        <Button
            Width="75"
            Margin="244,206,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Click="da"
            Content="Dodaj" />
        <Button
            Width="75"
            Margin="71,206,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Click="ne"
            Content="Odustani" />

    </Grid>
</Window>
