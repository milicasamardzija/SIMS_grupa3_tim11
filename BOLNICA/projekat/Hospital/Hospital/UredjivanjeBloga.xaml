<Window
    x:Class="Hospital.UredjivanjeBloga"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Hospital"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="UredjivanjeBloga"
    Width="600"
    Height="450"
    WindowStartupLocation="CenterScreen"
    mc:Ignorable="d">
    <Grid Background="#FF78D1D3">
        <TextBox x:Name="textBlog" Height="291" Margin="26,94,323.6,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top"
                 Background="LightGray"/>
        <Button Content="Sacuvaj izmene" HorizontalAlignment="Left" Margin="392,214,0,0" VerticalAlignment="Top" Width="98" Height="22"/>
        <Button Content="Odustani" Click="odustani" HorizontalAlignment="Left" Margin="392,284,0,0" VerticalAlignment="Top" Width="98" Height="25"/>
        <Label Content="Uredjenje bloga" HorizontalAlignment="Left" Margin="26,10,0,0" VerticalAlignment="Top" Height="38" Width="198" FontFamily="Lucida Handwriting"
            FontSize="20" />
        <Label Content="Trenutna obavestenja na blogu" HorizontalAlignment="Left" Margin="48,63,0,0" VerticalAlignment="Top" Height="31" Width="176"/>

    </Grid>
</Window>
